        <kctl name="Audio_Preamp1_Switch" value="IN_ADC3" />
        <kctl name="Audio_Preamp2_Switch" value="IN_ADC3" />
    </path>
    <path name="builtin_Mic_Mic1_Inverse" value="turnoff">
